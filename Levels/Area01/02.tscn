[node name="DialogInteraction" parent="NPC2" instance=ExtResource("10_s1pmn")]

[node name="DialogText" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "One-zero-one-zero... ah! No, no, no, that’s not balanced... where did I put that extra bit?

"
npc_info = ExtResource("8_yuemd")

[node name="DialogText2" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Uh... hello? Are you okay over there?
"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText3" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Oh! A traveler! Greetings, fellow data-wrangler! I am Binarist, the humble keeper of binary truths. 

"
npc_info = ExtResource("8_yuemd")

[node name="DialogText4" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "What brings you to my grove of glorious ones and zeroes?
"
npc_info = ExtResource("8_yuemd")

[node name="DialogText5" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "I’m just trying to figure out how to stop some Glitch thing from destroying Algorithmia.

"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText6" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "The Glitch, you say? A vile disruptor of balance! You’ll need sharp logic to defeat it.


"
npc_info = ExtResource("8_yuemd")

[node name="DialogText7" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "I’ll keep that in mind. By the way, what are you working on?
"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText8" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Oh, just a little contraption I call the Binary Balancer 3000. It ensures my binary trees are perfectly balanced... as all things should be.

"
npc_info = ExtResource("8_yuemd")

[node name="DialogText9" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Sounds... useful?"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText10" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Useful?! It’s a marvel of engineering! Say, [b]do you know what makes a binary tree balanced[/b]?!
"
npc_info = ExtResource("8_yuemd")

[node name="DialogChoice" type="Node" parent="NPC2/DialogInteraction"]
script = ExtResource("16_as0ht")
npc_info = ExtResource("8_yuemd")

[node name="DialogBranch" type="Node" parent="NPC2/DialogInteraction/DialogChoice"]
script = ExtResource("17_lam8a")
text = "Left and right heights differ by ≤ 1."
npc_info = ExtResource("8_yuemd")

[node name="DialogText" type="Node" parent="NPC2/DialogInteraction/DialogChoice/DialogBranch"]
script = ExtResource("11_ufet7")
text = "Brilliant! You’ve got the mind of a true binary artisan. Remember, balance is key in all things."
npc_info = ExtResource("8_yuemd")

[node name="DialogText2" type="Node" parent="NPC2/DialogInteraction/DialogChoice/DialogBranch"]
script = ExtResource("11_ufet7")
text = "Now, off you go! The Glitch awaits, and I have ones and zeroes to juggle!"
npc_info = ExtResource("8_yuemd")

[node name="DialogBranch2" type="Node" parent="NPC2/DialogInteraction/DialogChoice"]
script = ExtResource("17_lam8a")
text = "It has only one node."
npc_info = ExtResource("8_yuemd")

[node name="DialogText" type="Node" parent="NPC2/DialogInteraction/DialogChoice/DialogBranch2"]
script = ExtResource("11_ufet7")
text = "Geez! It's not a correct answer!"
npc_info = ExtResource("8_yuemd")

