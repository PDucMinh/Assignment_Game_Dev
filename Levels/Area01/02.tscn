[node name="DialogInteraction" parent="NPC" instance=ExtResource("10_s1pmn")]

[node name="DialogText" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Awake at last, I see! Welcome, young coder, to [b][color=red]Algorithmia—the land of computational wonder.[/color][/b] "
npc_info = ExtResource("8_fbvcf")

[node name="DialogText2" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "
I am [b][color=blue]Algo[/color][/b], your guide on this journey."
npc_info = ExtResource("8_fbvcf")

[node name="DialogText3" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Algorithmia? What happened? How did I get here?"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText4" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "You've been summoned by the Keeper of Logic to save our land. A terrible entity called [b][wave]The Glitch[/wave][/b] is wreaking havoc, corrupting our homes."
npc_info = ExtResource("8_fbvcf")

[node name="DialogText5" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Corrupted structures? That sounds... bad. What am I supposed to do?"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText6" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "You, my friend, are the chosen Debugger!"
npc_info = ExtResource("8_fbvcf")

[node name="DialogText7" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Your task is to journey through Algorithmia, restoring its data structures and collecting the fragments of the Great Algorithm. "
npc_info = ExtResource("8_fbvcf")

[node name="DialogText8" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Each region holds its own secrets and challenges."
npc_info = ExtResource("8_fbvcf")

[node name="DialogText9" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "How do I restore these data structures? Do I just fight things?"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText10" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Partly, yes. But battles alone won’t suffice. Knowledge is power here. "
npc_info = ExtResource("8_fbvcf")

[node name="DialogText11" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "You'll meet wise inhabitants who will test your understanding of data structures and algorithms."
npc_info = ExtResource("8_fbvcf")

[node name="DialogText12" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "Pass their quizzes to earn tools and power-ups!"
npc_info = ExtResource("8_fbvcf")

[node name="DialogText13" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "So… quizzes, battles, and fixing things. Got it. Where do I start?"
npc_info = ExtResource("12_2ebyk")

[node name="DialogText14" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "First, we must prepare you for the journey. Let me ask you something simple to warm up:"
npc_info = ExtResource("8_fbvcf")

[node name="DialogText15" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("11_ufet7")
text = "[b]'What is the depth of a binary tree with only a root node?' [/b] Choose your answer carefully."
npc_info = ExtResource("8_fbvcf")

[node name="DialogChoice" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("16_as0ht")
npc_info = ExtResource("8_fbvcf")

[node name="DialogBranch" type="Node" parent="NPC/DialogInteraction/DialogChoice"]
script = ExtResource("17_lam8a")
text = "1"
npc_info = ExtResource("8_fbvcf")

[node name="DialogText" type="Node" parent="NPC/DialogInteraction/DialogChoice/DialogBranch"]
script = ExtResource("11_ufet7")
text = "Excellent! You’re a fast learner. Let’s proceed. "
npc_info = ExtResource("8_fbvcf")

[node name="DialogText2" type="Node" parent="NPC/DialogInteraction/DialogChoice/DialogBranch"]
script = ExtResource("11_ufet7")
text = " Follow me—our first stop is the Binary Blossom Grove. "
npc_info = ExtResource("8_fbvcf")

[node name="DialogBranch2" type="Node" parent="NPC/DialogInteraction/DialogChoice"]
script = ExtResource("17_lam8a")
text = "0"
npc_info = ExtResource("8_fbvcf")

[node name="DialogText" type="Node" parent="NPC/DialogInteraction/DialogChoice/DialogBranch2"]
script = ExtResource("11_ufet7")
text = "Seriously?"
npc_info = ExtResource("8_fbvcf")

